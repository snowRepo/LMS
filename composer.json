{
    "name": "lms/library-management-system",
    "description": "A web-based Library Management System built with PHP",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.8",
        "tecnickcom/tcpdf": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "LMS\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
